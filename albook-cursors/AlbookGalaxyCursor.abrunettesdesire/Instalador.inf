; "AlbookGalaxy-abrunettesdesire" 

[Version]
signature="$CHICAGO$"

[DefaultInstall]
CopyFiles = Scheme.Cur, Scheme.Txt
AddReg    = Scheme.Reg

[DestinationDirs]
Scheme.Cur = 10,"%CUR_DIR%"
Scheme.Txt = 10,"%CUR_DIR%"

[Scheme.Reg]
HKCU,"Control Panel\Cursors\Schemes","%SCHEME_NAME%",,"%10%\%CUR_DIR%\%pointer%,%10%\%CUR_DIR%\%help%,%10%\%CUR_DIR%\%work%,%10%\%CUR_DIR%\%busy%,%10%\%CUR_DIR%\%Cross%,%10%\%CUR_DIR%\%Text%,%10%\%CUR_DIR%\%Hand%,%10%\%CUR_DIR%\%Unavailiable%,%10%\%CUR_DIR%\%Vert%,%10%\%CUR_DIR%\%Horz%,%10%\%CUR_DIR%\%Dgn1%,%10%\%CUR_DIR%\%Dgn2%,%10%\%CUR_DIR%\%move%,%10%\%CUR_DIR%\%alternate%,%10%\%CUR_DIR%\%link%"

; -- Common Information

[Scheme.Cur]
"cross.cur"
"diagonal derecha.ani"
"diagonal izquierda.ani"
"forbidden.cur"
"help.cur"
"horizontal.ani"
"pencil.cur"
"pointer.cur"
"pointer_watch.ani"
"pointing_hand.cur"
"size_all.cur"
"text.cur"
"up_arrow.cur"
"vertical.ani"
"watch.ani"
[Strings]
CUR_DIR		= "Albook Galaxy by LillyWeasley"
SCHEME_NAME	= "Albook Galaxy by LillyWeasley"
pointer		= "pointer.cur"
help		= "help.cur"
work		= "pointer_watch.ani"
busy		= "watch.ani"
cross		= "cross.cur"
text		= "text.cur"
hand		= "pencil.cur"
unavailiable	= "forbidden.cur"
vert		="vertical.ani"
horz		= "horizontal.ani"
dgn1		= "diagonal izquierda.ani"
dgn2		="diagonal derecha.ani"
move		="size_all.cur"
alternate		= "up_arrow.cur"
link		="pointing_hand.cur"